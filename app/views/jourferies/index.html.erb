<p id="notice"><%= notice %></p>

<h1>Jourferies</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jourferies.each do |jourfery| %>
      <tr>
        <td><%= jourfery.date %></td>
        <td><%= link_to 'Show', jourfery %></td>
        <td><%= link_to 'Edit', edit_jourfery_path(jourfery) %></td>
        <td><%= link_to 'Destroy', jourfery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jourfery', new_jourfery_path %>
