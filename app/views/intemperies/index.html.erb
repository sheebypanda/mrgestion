<p id="notice"><%= notice %></p>

<h1>Intemperies</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intemperies.each do |intempery| %>
      <tr>
        <td><%= intempery.date %></td>
        <td><%= link_to 'Show', intempery %></td>
        <td><%= link_to 'Edit', edit_intempery_path(intempery) %></td>
        <td><%= link_to 'Destroy', intempery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intempery', new_intempery_path %>
